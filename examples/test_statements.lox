
// Tenet Interpreter Test Suite - Chapter 8 Statements




// --- String Operations ---
print "Hello, World!";
print "Hello" + " " + "I am Tenet!";
print "The answer to everything is: " + "42";

// --- Basic Arithmetic ---
print 1 + 2;
print 10 - 3;
print 4 * 5;
print 20 / 4;

// --- Order of Operations ---
print 2 + 3 * 4;
print (2 + 3) * 4;
print 10 - 2 - 3;
print 100 / 10 / 2;

// --- Negative Numbers ---
print -5;
print --5;
print 10 + -3;
print -10 * -2;

// --- Comparison Operators ---
print 5 > 3;
print 5 < 3;
print 5 >= 5;
print 5 <= 4;
print 5 == 5;
print 5 != 3;

// --- Boolean Logic ---
print true;
print false;
print !true;
print !false;
print !!true;

// --- Nil ---
print nil;
print nil == nil;



// --- Mixed Expressions ---
print 1 + 2 + 3 + 4 + 5;
print (1 + 2) * (3 + 4);
print 2 * 3 + 4 * 5;

// --- Equality Comparisons ---
print 1 == 1;
print 1 == 2;
print "hello" == "hello";
print "hello" == "world";
print true == true;
print true == false;
print nil == nil;

// --- Inequality ---
print 1 != 2;
print 1 != 1;

// --- Complex Expressions ---
print (10 + 5) * 2 - 8 / 4;
print 100 / (5 + 5) * 2;

// --- Expression Statements (no print, just evaluate) ---
1 + 1;
"hello";
true;

// --- End of Test ---
print "All tests completed!";
