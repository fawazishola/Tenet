// Square Root using Babylonian Method
// This demonstrates custom math functions in Tenet

fun sqrt(n) {
  if (n < 0) return 0;
  var x = n;
  var y = 1;
  var e = 0.0001; // Precision
  while (x - y > e) {
    x = (x + y) / 2;
    y = n / x;
  }
  return x;
}

