// Simple tweak test
var reward = 3;

game SimplePD {
    players A, B
    strategies C, D
    
    payoff A {
        (C, C): reward
        (C, D): 0
        (D, C): 5
        (D, D): 1
    }
    
    payoff B {
        (C, C): reward
        (C, D): 5
        (D, C): 0
        (D, D): 1
    }
}

print "Tweak Test:";
tweak SimplePD { reward from 1 to 8 step 1 }
print "Done!";
